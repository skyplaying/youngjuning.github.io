{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngjuning/youngjuning.github.io.git",
    "branch": "main"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "@types/lodash.sortby": "^4.7.6",
    "antd": "^4.10.3",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.9",
    "lint-staged": "^10.0.7",
    "lodash.shuffle": "^4.2.0",
    "lodash.sortby": "^4.7.0",
    "postcss": "^8.2.8",
    "prettier": "^1.19.1",
    "sitemap": "^6.3.5",
    "yorkie": "^2.0.0"
  },
  "version": "0.0.1-0",
  "dependencies": {
    "gitalk": "^1.7.2"
  }
}
